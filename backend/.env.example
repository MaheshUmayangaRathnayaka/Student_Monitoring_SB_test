# Environment variables for SPMS Backend
# Copy this file to .env and fill in your actual values

# MongoDB Atlas connection string
MONGODB_URI=mongodb+srv://your-username:your-password@your-cluster.mongodb.net/spms?retryWrites=true&w=majority

# JWT Secret key (change this to a secure random string)
JWT_SECRET=myVerySecretJWTKey123456789AbCdEfGhIjKlMnOpQrStUvWxYz

# CORS allowed origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001